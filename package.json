{
  "name": "@rumsan/s3server",
  "version": "1.0.0",
  "description": "S3-compatible file server with REST API",
  "main": "dist/server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rumsan/s3server.git"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "example": "ts-node examples/client.ts",
    "docker:build": "docker build -t rumsan/s3server:latest .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t rumsan/s3server:dev .",
    "docker:run": "docker run -p 4568:4568 -v s3_storage:/app/storage rumsan/s3server:latest",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:compose:prod": "docker-compose -f docker-compose.dev.yml up s3server-prod -d"
  },
  "keywords": ["s3", "file-server", "rest-api", "docker", "aws-sdk"],
  "author": "Rumsan",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.621.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-xml-bodyparser": "^0.3.0",
    "multer": "^1.4.5-lts.1",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^10.0.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.10",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  }
}
